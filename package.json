{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "expo": "^27.0.1",
    "firebase": "^5.4.1",
    "hoist-non-react-statics": "^3.0.1",
    "prettier": "^1.14.2",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "react-navigation": "^2.12.1"
  },
  "devDependencies": {
    "@types/hoist-non-react-statics": "^3.0.1",
    "@types/expo": "^27.0.8",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.4.11",
    "@types/react-native": "^0.55.14",
    "@types/react-navigation": "^2.0.17",
    "@types/react-test-renderer": "^16.0.1",
    "jest-expo": "~27.0.0",
    "react-native-scripts-ts": "1.15.0",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.3.1",
    "ts-jest": "^22.4.6",
    "tslib": "^1.9.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3"
  },
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "prettier": "prettier --write '**/*.{css,json,ts,tsx}'",
    "test": "jest",
    "lint": "tslint --fix 'src/**/*.{ts,tsx}'"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
