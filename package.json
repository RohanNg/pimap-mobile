    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-native-gifted-chat": "^0.4.3",
    "react-native-modal-datetime-picker": "^6.0.0",
    "react-native-paper": "^2.2.4",
