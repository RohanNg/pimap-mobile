    "@types/expo__vector-icons": "^6.2.3",
    "@types/hoist-non-react-statics": "^3.0.1",
    "@types/immutable": "^3.8.7",
    "@types/jest": "^22.2.3",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.4.11",
    "@types/react-native": "^0.55.14",
    "@types/react-native-vector-icons": "^4.6.4",
    "@types/react-navigation": "^2.0.24",
